   <body style="background-color:#2C2F33 !important;color:white;font-family: 'Rubik', sans-serif;">

  <!-- Navigation -->
 <%- include("../extras/navbar",{page:"home"}) %>  
 
  <!-- Header -->
  <header class="bg-dark py-5 mb-5" style="background-color:#23272A!important;">
            <% var msg = ac.array()
        ac.forEach((msg, i) => { 
          
          if(i < 2) return; %>
      <div aria-live="polite" aria-atomic="true" role="alert" style="position: relative;">
        <div class="toast" style="position: absolute; top: 30px; right: 0; z-index: 1" data-delay="10000">
          <div class="toast-header">
            <img src="<%=msg.author.displayAvatarURL%>?size=64" style="border-radius:50%;width:64px;height:64px;" class="rounded mr-2" alt="...">
            <strong class="mr-auto"><%=msg.author.tag%></strong>
            <small><%= msg.createdAt.toDateString() %> </small>
            <button type="button" class="ml-2 mb-1 close" data-dismiss="toast" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <div class="toast-body" style="color:black;">
            <%- md.render(msg.cleanContent) %>
          </div>
          <% if(msg.attachments.array()[0]) { %>
          <div class="toast-header container">
            <img src="<%= msg.attachments.array()[0].url %>">
          </div>
          <% } %>
          <div class="toast-header">
            Discord Labs Announcement
          </div>
        </div>
      </div>
      <% })%>   
    <div class="container h-100">
      <div class="row h-100 align-items-center">
        <div class="col-lg-12">
          <h1 class="display-4 text-white mt-5 mb-2">Discord Labs</h1>
          <p class="lead mb-5 text-white-50">Creating the next generation of Discord sites and tools.</p>
        </div>
      </div>
    </div>

  </header>

  <!-- Page Content -->
  <div class="container" id="services">

    <div class="row" >
      <div class="col-md-8 mb-5">
        <h2>What We Do</h2>
        <hr style="border-color:white;">
        <p>At Discord Labs, we strive to make your Discord experience as great as possible, and our Next Generation initiative is the way we do it. 
          We have a wide range of products in services ranging from <a href="//statcord.com" target="_blank" class="ilink">Growing your Discord Bot</a> to 
          <a href="//usercord.org" target="_blank" class="ilink">Unlocking the full potential of your Discord Profile.</a> 
         
      </div>
      <div class="col-md-4 mb-5">
        <h2>Contact Us</h2>
        <hr style="border-color:white;">
        <address>
          <a href="mailto:support@discordlabs.org" class="ulink"><i class="fas fa-envelope"></i> support@discordlabs.org</a>
        </address>
      </div>
    </div>
    <div class="row">
      <div class="col-md-4 mb-5">
        <div class="card h-100" style="border: 4px solid #e2a52d;background-color:#23272A !important;">
          <img class="card-img-top" src="//cdn.discordlabs.org/logo200.png" alt="">
          <div class="card-body">
            <h4 class="card-title" style="text-align:center;">Discord Bot Labs</h4>
            <p class="card-text">A great way to find amazing Discord bots for your Discord server. Also a great way to grow your Discord bot.</p>
          </div>
          <div class="card-footer" style="text-align:center;border-color:white;">
            <a href="//bots.discordlabs.org" class="btn btn-primary btn-lg">Visit Discord Bot Labs</a>
          </div>
        </div>
      </div>
      <div class="col-md-4 mb-5">
